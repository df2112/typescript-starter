{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"],
      "@lib/*": ["lib/*"],
      "@brokers/*": ["src/brokers/*"],
      "@ibkr/*": ["src/brokers/ibkr/*"],
      "@tasty/*": ["src/brokers/tasty/*"],
    }
  }
}
